{
  "name": "qc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "doppler run -- next dev --turbopack",
    "dev:local": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "knip": "knip",
    "build:ios": "STATIC_EXPORT=true doppler run -- npm run build && npx cap sync ios",
    "ios": "npm run build:ios && npx cap open ios",
    "check": "npm run lint && npm run typecheck && npm run format:check && npm run test"
  },
  "dependencies": {
